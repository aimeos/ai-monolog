{
	"name": "aimeos/ai-monolog",
	"description": "Monolog adapter for Aimeos e-commerce solutions",
	"keywords": ["aimeos", "adapter", "monolog", "log", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-monolog",
		"issues": "https://github.com/aimeos/ai-monolog/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.0.11",
		"monolog/monolog": "~2.0||~3.0",
		"aimeos/aimeos-core": "2023.10.*"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.0||~10.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	}
}
